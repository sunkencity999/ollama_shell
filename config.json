{
    "default_model": "llama3.2:latest",
    "default_vision_model": "llama3.2-vision:latest",
    "verbose": true,
    "save_history": true,
    "history_file": "~/.ollama_shell_history",
    "show_model_details": true,
    "temperature": 0.8,
    "context_length": 8192,
    "knowledge_base": {
        "enabled": true,
        "path": "~/.ollama_shell_kb",
        "max_results": 5,
        "similarity_threshold": 0.05
    },
    "stored_prompts": {}
}